    "react-qrcode-logo": "^4.0.0",
    "react-redux": "^8.0.5",
    "react-select": "^5.8.0",
    "react-toastify": "^9.1.1",
    "redux-persist": "^6.0.0",
    "sharp": "^0.34.4",
    "swiper": "^8.4.5",
    "tailwindcss": "^3.2.4",
    "turbo": "^1.9.0",
    "yup": "^1.0.0"
  },
  "transformIgnorePatterns": [
    "node_modules/(?!(react-toastify))"
  ],
  "resolutions": {
    "braces": ">=3.0.3",
    "cross-spawn": ">=7.0.5",
    "ws": ">=7.5.10"
  },
  "author": {
    "name": "ShipLink",
    "email": "info@ship.link",
    "url": "https://ship.link/"
  },
  "engines": {
    "node": ">=20.0.0",
    "yarn": ">=1.22.0",
    "npm": "please-use-yarn"
  },
  "license": "MIT"
}
