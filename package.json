{
  "name": "shiplink-basic-project",
  "private": true,
  "version": "1.0.1",
  "description": "A NextJS application",
  "scripts": {
    "develop": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "server": "next build && next start",
    "lint": "next lint",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --write",
    "lint:staged": "prettier . --write && eslint . --fix && pretty-quick --staged",
    "prepare": "husky install",
    "release": "standard-version --no-verify",
    "release:first": "standard-version --no-verify -- --first-release",
    "release:minor": "standard-version --no-verify --release-as minor",
    "release:patch": "standard-version --no-verify --release-as patch",
    "release:major": "standard-version --no-verify --release-as major",
    "clear-all": "rm -rf .next node_modules out",
    "restart": "rm -rf .next node_modules out && yarn install && yarn develop",
    "rebuild": "rm -rf .next node_modules out && yarn install && yarn build",
    "turbo-build": "turbo build --force",
    "postbuild": "next-sitemap",
    "knip": "knip"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.64.1",
    "@opentelemetry/exporter-jaeger": "^2.1.0",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.205.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.205.0",
    "@opentelemetry/instrumentation-http": "^0.205.0",
    "@opentelemetry/resources": "^2.1.0",
    "@opentelemetry/sdk-metrics": "^2.1.0",
    "@opentelemetry/sdk-node": "^0.205.0",
    "@opentelemetry/sdk-trace-node": "^2.1.0",
    "@opentelemetry/semantic-conventions": "^1.27.0",
    "@opentelemetry/winston-transport": "^0.16.0",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "autoprefixer": "^10.4.13",
    "commitlint-plugin-function-rules": "^4.0.2",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^5.2.0",
    "file-loader": "^6.2.0",
    "husky": "^9.1.7",
    "knip": "^5.63.1",
    "postcss": "^8.4.20",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "pretty-quick": "^4.0.0",
    "react-linkify": "^1.0.0-alpha",
    "standard-version": "^9.5.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.22.5",
    "@hookform/resolvers": "^5.2.2",
    "@iconscout/react-unicons": "^2.2.4",
    "@iconscout/react-unicons-solid": "^1.0.2",
    "@microsoft/signalr": "^7.0.9",
    "@newrelic/browser-agent": "^1.285.0",
    "@redux-devtools/extension": "^3.2.5",
    "@reduxjs/toolkit": "^1.9.2",
    "@svgr/webpack": "^6.5.1",
    "@tailwindcss/typography": "^0.5.13",
    "axios": "^1.8.2",
