    "eslint": "^8.30.0",
    "eslint-config-next": "15.5.3",
    "howler": "^2.2.4",
    "html-react-parser": "^5.2.6",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.0",
    "leaflet": "^1.9.4",
    "lodash": "^4.17.21",
    "lottie-react": "^2.4.0",
    "next": "15.5.3",
    "next-sitemap": "^4.0.7",
    "pluralize": "^8.0.0",
    "prop-types": "^15.8.1",
    "react": "19.1.1",
    "react-countdown": "^2.3.5",
    "react-country-flag": "^3.1.0",
    "react-date-range": "^1.4.0",
    "react-dom": "19.1.1",
    "react-dropzone": "^14.2.3",
    "react-google-recaptcha": "^3.1.0",
