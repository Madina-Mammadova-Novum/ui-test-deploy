{
  "name": "shiplink-basic-project",
  "private": true,
  "version": "1.0.1",
  "description": "A NextJS application",
  "scripts": {
    "develop": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "server": "next build && next start",
    "lint": "next lint",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --write",
    "lint:staged": "prettier . --write && eslint . --fix && pretty-quick --staged",
    "prepare": "husky install",
    "release": "standard-version --no-verify",
    "release:first": "standard-version --no-verify -- --first-release",
    "release:minor": "standard-version --no-verify --release-as minor",
    "release:patch": "standard-version --no-verify --release-as patch",
    "release:major": "standard-version --no-verify --release-as major",
    "clear-all": "rm -rf .next node_modules out",
    "restart": "rm -rf .next node_modules out && yarn install && yarn develop",
    "rebuild": "rm -rf .next node_modules out && yarn install && yarn build",
    "turbo-build": "turbo build --force",
    "postbuild": "next-sitemap"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@testing-library/react": "^13.4.0",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "autoprefixer": "^10.4.13",
    "babel-eslint": "^10.1.0",
    "commitlint-plugin-function-rules": "^1.7.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.7",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.9.1",
    "file-loader": "^6.2.0",
    "husky": "^8.0.0",
    "json": "^11.0.0",
    "postcss": "^8.4.20",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "pretty-quick": "^4.0.0",
    "react-linkify": "^1.0.0-alpha",
    "standard-version": "^9.5.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.22.5",
    "@hookform/devtools": "^4.3.0",
    "@hookform/resolvers": "2.8.5",
    "@iconscout/react-unicons-solid": "^1.0.2",
    "@microsoft/signalr": "^7.0.9",
    "@opentelemetry/auto-instrumentations-node": "^0.52.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.53.0",
    "@opentelemetry/instrumentation-http": "^0.54.1",
    "@opentelemetry/resources": "^1.26.0",
    "@opentelemetry/sdk-node": "^0.53.0",
    "@opentelemetry/sdk-trace-node": "^1.26.0",
    "@opentelemetry/semantic-conventions": "^1.27.0",
    "@redux-devtools/extension": "^3.2.5",
    "@reduxjs/toolkit": "^1.9.2",
    "@svgr/webpack": "^6.5.1",
    "@tailwindcss/typography": "^0.5.13",
    "axios": "^1.6.5",
    "classnames": "^2.3.2",
    "date-fns": "^2.29.3",
    "dlv": "^1.1.3",
    "eslint": "^8.30.0",
    "eslint-config-next": "^13.0.6",
    "form-data": "^4.0.0",
    "howler": "^2.2.4",
    "html-react-parser": "^3.0.15",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.0",
    "leaflet": "^1.9.4",
    "lottie-react": "^2.4.0",
    "next": "^14.0.4",
    "next-sitemap": "^4.0.7",
    "pluralize": "^8.0.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-countdown": "^2.3.5",
    "react-country-flag": "^3.1.0",
    "react-date-range": "^1.4.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-google-recaptcha": "^3.1.0",
    "react-hook-form": "7.42.0",
    "react-leaflet": "^4.2.1",
    "react-paginate": "^8.1.5",
    "react-phone-input-2": "^2.15.1",
    "react-redux": "^8.0.5",
    "react-select": "^5.8.0",
    "react-toastify": "^9.1.1",
    "redux-persist": "^6.0.0",
    "swiper": "^8.4.5",
    "tailwindcss": "^3.2.4",
    "turbo": "^1.9.0",
    "yup": "^1.0.0"
  },
  "transformIgnorePatterns": [
    "node_modules/(?!(react-toastify))"
  ],
  "author": {
    "name": "ShipLink",
    "email": "info@ship.link",
    "url": "https://ship.link/"
  },
  "engines": {
    "node": "20.x",
    "npm": "10.x"
  },
  "license": "MIT"
}
